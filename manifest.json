

{
"manifest_version": 3,
"name": "Sidebar ChatGPT",
"version": "1.0",
"description": "Access ChatGPT in a sidebar-like window.",
"permissions": ["activeTab", "storage"],
"background": {
    "service_worker": "background.js"
},
"action": {
    "default_popup": "popup.html",
    "default_icon": {
        "16": "icons/icon2.png",
        "48": "icons/icon2.png",
        "128": "icons/icon2.png"
    }
},
"icons": {
    "16": "icons/icon2.png",
    "48": "icons/icon2.png",
    "128": "icons/icon2.png"
}
}